<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ApplicationIcon />
    <StartupObject />
    <Authors>Kiritsu and contributors</Authors>
    <Company></Company>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>3.2.0</Version>
    <Description>.NET Standard wrapper for the osu!api</Description>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <ProjectUrl>https://github.com/Kiritsu/OsuSharp</ProjectUrl>
    <AssemblyVersion>3.2.0.0</AssemblyVersion>
    <FileVersion>3.2.0.0</FileVersion>
    <PackageProjectUrl>https://github.com/Kiritsu/OsuSharp</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/Kiritsu/OsuSharp/blob/master/LICENSE.md</PackageLicenseUrl>
    <LangVersion>7.1</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>
